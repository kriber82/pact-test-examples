{
  "consumer": {
    "name": "unique-name-of-my-spa"
  },
  "provider": {
    "name": "api-identifier"
  },
  "interactions": [
    {
      "description": "a request to GET a user",
      "providerState": "user 1 exists",
      "request": {
        "method": "GET",
        "path": "/api/users/1"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "id": 1,
          "firstName": "Toto",
          "lastName": "Titi"
        },
        "matchingRules": {
          "$.body.firstName": {
            "match": "type"
          },
          "$.body.lastName": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to GET all available user",
      "providerState": "users one and two exist",
      "request": {
        "method": "GET",
        "path": "/api/users"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "users": [
            {
              "id": 1,
              "firstName": "Toto",
              "lastName": "Titi"
            },
            {
              "id": 2,
              "firstName": "Tata",
              "lastName": "Tütü"
            }
          ]
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}